<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <servlet>
        <servlet-name>FileUpload</servlet-name>
        <servlet-class>com.telusko.FileUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileDownload</servlet-name>
        <servlet-class>com.telusko.FileDownload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileOpen</servlet-name>
        <servlet-class>com.telusko.FileOpen</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateDoc</servlet-name>
        <servlet-class>com.telusko.CreateDoc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DocDel</servlet-name>
        <servlet-class>com.telusko.DocDel</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FileUpload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileDownload</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileOpen</servlet-name>
        <url-pattern>/open</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateDoc</servlet-name>
        <url-pattern>/create</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DocDel</servlet-name>
        <url-pattern>/delete</url-pattern>
    </servlet-mapping>

</web-app>